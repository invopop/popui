package examples

import (
	popui "github.com/invopop/popui/go"
	"github.com/invopop/popui/go/props"
)

templ ButtonAnchorExample() {
	<div class="flex gap-2">
		@popui.AnchorButton(props.AnchorButton{
			Href:    "/docs",
			Variant: "primary",
		}) {
			Visit Website
		}
		@popui.AnchorButton(props.AnchorButton{
			Href:    "/docs",
			Variant: "secondary",
			Target:  "_blank",
		}) {
			Open in New Tab
		}
	</div>
}
