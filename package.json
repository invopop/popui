{
  "name": "@invopop/ui-kit",
  "license": "MIT",
  "version": "0.0.76",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-web-components": "vite build --config vite.wbc.config.ts",
    "publish-wbc": "npm run build-web-components && cp ./package-wbc.json ./dist/package.json && cd ./dist && npm run publish"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./twTheme.js": "./dist/tw.theme.js",
    "./package.json": "./dist/package.json"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-interactions": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/blocks": "^7.4.6",
    "@storybook/svelte": "^7.4.6",
    "@storybook/sveltekit": "^7.4.6",
    "@storybook/testing-library": "^0.2.2",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@sveltejs/package": "^2.0.0",
    "@tailwindcss/forms": "^0.5.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-svelte": "^2.30.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "publint": "^0.1.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.4.6",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "svelte-headlessui": "^0.0.32",
    "tslib": "^2.4.1",
    "typescript": "^4.1.6",
    "vite": "^4.4.2",
    "vitest": "^0.34.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@beyonk/svelte-datepicker": "^13.0.3",
    "@floating-ui/core": "^1.5.1",
    "@invopop/ui-icons": "0.0.22",
    "@steeze-ui/heroicons": "^2.2.3",
    "@steeze-ui/svelte-icon": "^1.5.0",
    "@twind/core": "^1.1.3",
    "@twind/preset-autoprefix": "^1.0.7",
    "@twind/preset-tailwind": "^1.1.4",
    "@twind/preset-tailwind-forms": "^1.1.2",
    "@twind/with-web-components": "^1.1.3",
    "@types/flatpickr": "^3.1.2",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.10",
    "flatpickr": "^4.6.13",
    "inter-ui": "^3.19.3",
    "svelte-floating-ui": "^1.5.8",
    "svelte-intersection-observer-action": "^0.0.4",
    "svelte-transition": "^0.0.10",
    "twind": "^0.16.19"
  }
}
